{
  "family": "server_production_ai4cod",
  "containerDefinitions": [
    {
      "name": "ai4cod",
      "image": "623386377432.dkr.ecr.us-east-2.amazonaws.com/ai4cod.com:latest",
      "cpu": 1000,
      "memory": 1000,
      "portMappings": [
        {
          "name": "server",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "entryPoint": [
        "./devops/entrypoint/server.sh"
      ],
      "command": [],
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "POSTGRESQL_USERNAME_FULL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/full/username::"
        },
        {
          "name": "POSTGRESQL_PASSWORD_FULL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/full/password::"
        },
        {
          "name": "POSTGRESQL_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/standard/username::"
        },
        {
          "name": "POSTGRESQL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/standard/password::"
        },
        {
          "name": "POSTGRESQL_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/host/all::"
        },
        {
          "name": "POSTGRESQL_PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rds/port/all::"
        },
        {
          "name": "SECRET_KEY_BASE",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:rails/secret/all::"
        },
        {
          "name": "STORAGE_REGION",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:s3/region/all::"
        },
        {
          "name": "STORAGE_BUCKET",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:s3/bucket/all::"
        },
        {
          "name": "STORAGE_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:iam/access/key::"
        },
        {
          "name": "STORAGE_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:iam/access/secret::"
        },
        {
          "name": "RAILS_CONCURRENCY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:server/concurrency/production::"
        },
        {
          "name": "RAILS_MAX_THREADS",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:server/threads/production::"
        },
        {
          "name": "RACK_TIMEOUT_SERVICE_TIMEOUT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:server/timeout/production::"
        },
        {
          "name": "REDIS_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:redis/url/all::"
        },
        {
          "name": "SENTRY_DSN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:sentry/dsn/all::"
        },
        {
          "name": "SENTRY_RATE",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:sentry/rate/production::"
        },
        {
          "name": "NEW_RELIC_LICENSE_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:newrelic/license/all::"
        },
        {
          "name": "NEW_RELIC_USER_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:newrelic/user/all::"
        },
        {
          "name": "SMTP_ADDRESS",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:smtp/address/all::"
        },
        {
          "name": "SMTP_DOMAIN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:smtp/domain/all::"
        },
        {
          "name": "SMTP_PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:smtp/port/all::"
        },
        {
          "name": "SMTP_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:smtp/username/all::"
        },
        {
          "name": "SMTP_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:623386377432:secret:ai4cod.com-H0Tr1N:smtp/password/all::"
        }
      ],
      "startTimeout": 2,
      "stopTimeout": 2,
      "interactive": true,
      "pseudoTerminal": true,
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/server_production_ai4cod",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "sh",
          "-c",
          "ps aux | grep '[p]uma'"
        ],
        "interval": 5,
        "timeout": 2,
        "retries": 3,
        "startPeriod": 2
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::623386377432:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::623386377432:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
